# Configuración de Staging para Web-Admin
NEXT_PUBLIC_ENVIRONMENT=staging
NODE_ENV=staging

# URLs de APIs - Ambiente Staging (Pre-producción avanzado)
NEXT_PUBLIC_API_URL=https://api-staging.urbantracker.com
NEXT_PUBLIC_BACKEND_URL=https://backend-staging.urbantracker.com:8080

# Mapas y Geolocalización
NEXT_PUBLIC_MAPBOX_TOKEN=pk.eyJ1IjoiYWZzYjExNCIsImEiOiJjbWI1bmN2OGYxanloMmlvbjd0dndtb3g5In0.2ON4hP04tvToiU_p_IsHbg

# Configuración de la aplicación
NEXT_PUBLIC_APP_NAME=UrbanTracker Admin - Staging
NEXT_PUBLIC_APP_VERSION=1.0.0-staging
NEXT_PUBLIC_COMPANY_NAME=SENA

# Configuración de Staging (similar a producción con logging extendido)
NEXT_PUBLIC_DEBUG=false
NEXT_PUBLIC_LOG_LEVEL=info

# Timeouts y configuración de red para Staging
NEXT_PUBLIC_API_TIMEOUT=12000
NEXT_PUBLIC_RETRY_ATTEMPTS=2

# Configuración de autenticación (Staging)
NEXT_PUBLIC_SESSION_TIMEOUT=3600000
NEXT_PUBLIC_TOKEN_REFRESH_INTERVAL=300000

# Puertos de Staging
NEXT_PUBLIC_DEV_PORT=3005
NEXT_PUBLIC_API_PORT=8080

# Configuraciones específicas de Staging
NEXT_PUBLIC_ENABLE_ANALYTICS=true
NEXT_PUBLIC_SECURITY_HEADERS=true
NEXT_PUBLIC_CACHE_DURATION=7200
NEXT_PUBLIC_ASSET_OPTIMIZATION=true
NEXT_PUBLIC_STAGING_MODE=true
NEXT_PUBLIC_PERFORMANCE_MONITORING=true
NEXT_PUBLIC_ERROR_REPORTING=true